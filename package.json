{
  "name": "libp2p-kad-dht-viz",
  "version": "0.0.0",
  "description": "Visualizer of the Kad-DHT for libp2p",
  "main": "src/index.js",
  "scripts": {
    "lint": "aegir lint",
    "test": "aegir test -t node",
    "test:node": "aegir test -t node",
    "build": "aegir build",
    "docs": "aegir docs",
    "release": "aegir release --docs -t node",
    "release-minor": "aegir release --type minor --docs -t node",
    "release-major": "aegir release --type major --docs -t node",
    "coverage": "aegir coverage",
    "coverage-publish": "aegir-coverage publish",
    "watch": "watchify -v --debug src/javascripts -o src/sim.js"
  },
  "pre-push": [
    "lint",
    "test"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/libp2p/js-libp2p-kad-dht-viz.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/libp2p/js-libp2p-kad-dht-viz/issues"
  },
  "engines": {
    "node": ">=6.0.0",
    "npm": ">=3.0.0"
  },
  "homepage": "https://github.com/libp2p/js-libp2p-kad-dht-viz",
  "dependencies": {
    "@nodeutils/defaults-deep": "^1.1.0",
    "base32.js": "~0.1.0",
    "bignumber.js": "^8.1.1",
    "bs58": "^4.0.1",
    "chai-checkmark": "^1.0.1",
    "d3": "^5.9.2",
    "debug": "^4.1.1",
    "err-code": "^1.1.2",
    "moment": "^2.24.0"
  },
  "devDependencies": {
    "aegir": "^18.1.1",
    "chai": "^4.2.0",
    "dirty-chai": "^2.0.1",
    "lodash": "^4.17.11",
    "lodash.random": "^3.2.0",
    "lodash.range": "^3.2.0",
    "sinon": "^7.2.4",
    "watchify": "^3.11.1"
  }
